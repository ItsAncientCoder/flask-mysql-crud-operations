    "import win32com.client as winc1\n",
    "import easygui\n",
    "\n",
    "speak = winc1.Dispatch('SAPI.SpVoice')\n",
    "\n",
    "while(True):\n",
    "    text = easygui.enterbox('Type Text here..')\n",
    "    if text!='0':\n",
    "        speak.Speak(text)\n",
    "    else:\n",
    "        break;"
