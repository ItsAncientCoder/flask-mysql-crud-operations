    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "url = 'https://www.python.org/~guido/'\n",
    "r = requests.get(url)\n",
    "html_doc = r.content\n",
    "soup = BeautifulSoup(html_doc, 'html.parser')\n",
    "for a in soup.find_all('a'):\n",
    "    href = a.get('href')\n",
    "    if href.startswith('http'):\n",
    "        print(href)"
   ]
