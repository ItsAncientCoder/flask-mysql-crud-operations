    "input_string = input('Enter number sep by space')\n",
    "nums = input_string.split()\n",
    "nums = list(map(int, nums))\n",
    "for i in nums:\n",
    "    op = ''\n",
    "    while i>0:\n",
    "        op += '*'\n",
    "        i = i-1\n",
    "    print(op)"
